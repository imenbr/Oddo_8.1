<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>
<!--**************           Resuest for quotation sequence             ****************-->
        <record id="seq_request_for_quotation_type" model="ir.sequence.type">
            <field name="name">request for quotation</field>
            <field name="code">request.quotation</field>
        </record>
        
        <record id="seq_request_for_quotation" model="ir.sequence">
            <field name="name">request for quotation</field>
            <field name="code">request.quotation</field>
            <field eval="4" name="padding"/>
            <field name="prefix">DP%(year)s/</field>
        </record>

    </data>
</openerp>
